<script>
  document.addEventListener('DOMContentLoaded', () => {
    const sidebarButtons = document.querySelectorAll('.collections-sidebar-nav .collections-sidebar-button');
    const galleryContents = document.querySelectorAll('.collections-gallery-content-area');
    
    function showGallery(targetId) {
      // Remove active from all sidebar buttons
      sidebarButtons.forEach(button => button.classList.remove('active'));
      
      // Hide all galleries
      galleryContents.forEach(content => content.classList.remove('active'));
      
      // Hide the no-gallery message
      const noGalleryMessage = document.getElementById('no-gallery-selected');
      if (noGalleryMessage) {
        noGalleryMessage.style.display = 'none';
      }
      
      // Add active to the corresponding sidebar button
      const activeButton = document.querySelector(`[data-target="${targetId}"]`);
      if (activeButton) {
        activeButton.classList.add('active');
      }
      
      // Show target gallery
      const targetContent = document.getElementById(targetId);
      if (targetContent) {
        targetContent.classList.add('active');
      }
    }
    
    function showNoGalleryMessage() {
      // Hide all galleries
      galleryContents.forEach(content => content.classList.remove('active'));
      
      // Show the no-gallery message
      const noGalleryMessage = document.getElementById('no-gallery-selected');
      if (noGalleryMessage) {
        noGalleryMessage.style.display = 'flex';
      }
    }
    
    // Handle sidebar buttons (excluding dropdown triggers and submenu items)
    sidebarButtons.forEach(button => {
      if (!button.classList.contains('collections-dropdown-trigger') && !button.classList.contains('submenu-item')) {
        button.addEventListener('click', (e) => {
          e.preventDefault();
          const targetId = button.dataset.target;
          
          // Best Of should show the gallery directly
          if (targetId === 'sports-bestof-gallery') {
            showGallery(targetId);
          } else {
            // For other category buttons, show "Choisir une galerie" message
            showNoGalleryMessage();
          }
        });
      }
    });
    
    // Handle dropdown toggles
    const dropdownTriggers = document.querySelectorAll('.collections-dropdown-trigger');
    dropdownTriggers.forEach(trigger => {
      trigger.addEventListener('click', (e) => {
        e.preventDefault();
        e.stopPropagation();
        
        // Remove active state from all buttons
        sidebarButtons.forEach(button => button.classList.remove('active'));
        
        // Add active state to clicked dropdown trigger
        trigger.classList.add('active');
        
        const submenu = trigger.nextElementSibling;
        if (submenu) {
          // Close all other dropdowns first
          document.querySelectorAll('.collections-dropdown-submenu').forEach(menu => {
            if (menu !== submenu) {
              menu.classList.remove('active');
            }
          });
          // Toggle the clicked dropdown
          submenu.classList.toggle('active');
        }
        
        // Show "Choisir une galerie" message when clicking category buttons
        showNoGalleryMessage();
      });
    });
    
    // Handle submenu items
    const submenuItems = document.querySelectorAll('.submenu-item');
    submenuItems.forEach(item => {
      item.addEventListener('click', (e) => {
        showGallery(item.dataset.target);
      });
    });

    // Mobile dropdown functionality
    const sportsCategorySelect = document.getElementById('sports-category-select');
    const sportsSubcategorySelect = document.getElementById('sports-subcategory-select');
    
    // Define subcategories for each category
    const subcategories = {
      'best-of': [
        { value: 'sports-bestof-gallery', text: 'Best Of' }
      ],
      'sports-nautiques': [
        { value: 'sports-surf-gallery', text: 'Surf' },
        { value: 'sports-kitesurf-gallery', text: 'Kite Surf' },
        { value: 'sports-wakeboard-gallery', text: 'Wakeboard' },
        { value: 'sports-apnee-gallery', text: 'Apnée' },
        { value: 'sports-kayak-gallery', text: 'Kayak' },
        { value: 'sports-paddleboard-gallery', text: 'Paddle Board' },
        { value: 'sports-funboard-gallery', text: 'Funboard' },
        { value: 'sports-voile-gallery', text: 'Voile' },
        { value: 'sports-wakefoil-gallery', text: 'Wakefoil' },
        { value: 'sports-wakesurf-gallery', text: 'Wake Surf' }
      ],
      'sports-hiver': [
        { value: 'sports-ski-freeride-gallery', text: 'Ski Freeride' },
        { value: 'sports-ice-climbing-gallery', text: 'Ice Climbing' },
        { value: 'sports-musher-gallery', text: 'Musher' },
        { value: 'sports-raquette-gallery', text: 'Raquette' },
        { value: 'sports-skicross-gallery', text: 'Ski Cross' },
        { value: 'sports-snowboard-freeride-gallery', text: 'Snowboard Freeride' },
        { value: 'sports-snowhawk-gallery', text: 'Snowhawk' }
      ],
      'sports-terrestres': [
        { value: 'sports-trail-gallery', text: 'Trail' },
        { value: 'sports-vtt-gallery', text: 'VTT' },
        { value: 'sports-escalade-gallery', text: 'Escalade' },
        { value: 'sports-equitation-gallery', text: 'Équitation' },
        { value: 'sports-golf-gallery', text: 'Golf' },
        { value: 'sports-longboard-gallery', text: 'Longboard' },
        { value: 'sports-nordic-walking-gallery', text: 'Nordic Walking' },
        { value: 'sports-skateboard-gallery', text: 'Skateboard' }
      ],
      'sports-aeriens': [
        { value: 'sports-parapente-gallery', text: 'Parapente' },
        { value: 'sports-wingsuit-gallery', text: 'Wingsuit' }
      ],
      'sports-collectifs': [
        { value: 'sports-football-gallery', text: 'Football' },
        { value: 'sports-rugby-gallery', text: 'Rugby' },
        { value: 'sports-beach-rugby-gallery', text: 'Beach Rugby' },
        { value: 'sports-beach-volley-gallery', text: 'Beach Volley' }
      ],
      'sports-mecaniques': [
        { value: 'sports-motocross-gallery', text: 'Motocross' },
        { value: 'sports-fmx-gallery', text: 'FMX' },
        { value: 'sports-hill-climbing-gallery', text: 'Hill Climbing' },
        { value: 'sports-trial-gallery', text: 'Trial' },
        { value: 'sports-moto-custom-gallery', text: 'Moto Custom' },
        { value: 'sports-moto-gp-gallery', text: 'Moto GP' }
      ]
    };

    // Update subcategory dropdown when category changes
    if (sportsCategorySelect && sportsSubcategorySelect) {
      sportsCategorySelect.addEventListener('change', (event) => {
        const selectedCategory = event.target.value;
        
        if (!selectedCategory) {
          // If empty selection, hide subcategory dropdown
          sportsSubcategorySelect.style.display = 'none';
          return;
        }
        
        const subcategoryOptions = subcategories[selectedCategory] || [];
        
        // Clear and populate subcategory dropdown
        sportsSubcategorySelect.innerHTML = '';
        
        // Show/hide second dropdown based on number of subcategories
        if (subcategoryOptions.length > 1) {
          // Show subcategory dropdown for categories with multiple options
          sportsSubcategorySelect.style.display = 'block';
          // Add placeholder option
          const placeholderOption = document.createElement('option');
          placeholderOption.value = '';
          placeholderOption.textContent = 'Choisir une galerie...';
          placeholderOption.selected = true; // Make sure it's selected
          sportsSubcategorySelect.appendChild(placeholderOption);
          
          subcategoryOptions.forEach(option => {
            const optionElement = document.createElement('option');
            optionElement.value = option.value;
            optionElement.textContent = option.text;
            sportsSubcategorySelect.appendChild(optionElement);
          });
          
          // Show "Choisir une galerie" message when category is selected (unless we have a hash)
          if (!window.location.hash) {
            showNoGalleryMessage();
          }
        } else {
          // Hide subcategory dropdown for categories with only one option (like Best Of)
          sportsSubcategorySelect.style.display = 'none';
          // Show "Choisir une galerie" message instead of showing Best Of directly (unless we have a hash)
          if (!window.location.hash) {
            showNoGalleryMessage();
          }
        }
      });

      // Handle subcategory selection
      sportsSubcategorySelect.addEventListener('change', (event) => {
        if (event.target.value) {
          showGallery(event.target.value);
        } else {
          // Show "Choisir une galerie" message when "Choisir ..." is selected
          showNoGalleryMessage();
        }
      });
      
      // Handle initial load and hash navigation
      if (window.location.hash) {
        const hashId = window.location.hash.substring(1);
        console.log('Hash navigation for:', hashId);
        
        // Check if it's a gallery hash
        if (document.getElementById(hashId)) {
          console.log('Found gallery element for:', hashId);
          showGallery(hashId);
          
          // Update mobile dropdowns to match the hash
          for (const [category, galleries] of Object.entries(subcategories)) {
            const galleryFound = galleries.find(g => g.value === hashId);
            if (galleryFound) {
              // Update mobile dropdowns
              sportsCategorySelect.value = category;
              // Trigger the change event to update subcategory dropdown
              const changeEvent = new Event('change');
              sportsCategorySelect.dispatchEvent(changeEvent);
              if (subcategories[category].length > 1) {
                sportsSubcategorySelect.value = hashId;
              }
              break;
            }
          }
        } 
        // Check if it's a category hash (like #sports-nautiques)
        else if (subcategories[hashId]) {
          console.log('Found category in subcategories for:', hashId);
          // Show "Choisir une galerie" message for category hashes
          showNoGalleryMessage();
          
          // Update mobile dropdowns
          sportsCategorySelect.value = hashId;
          const changeEvent = new Event('change');
          sportsCategorySelect.dispatchEvent(changeEvent);
          
          // Update desktop sidebar
          // Remove active state from all buttons
          sidebarButtons.forEach(button => button.classList.remove('active'));
          
          // Find and activate the category dropdown trigger
          const categoryTrigger = document.querySelector(`[data-target="${hashId}"]`);
          if (categoryTrigger) {
            categoryTrigger.classList.add('active');
            
            // Open the dropdown if it has subcategories
            if (subcategories[hashId].length > 1) {
              const submenu = categoryTrigger.nextElementSibling;
              if (submenu) {
                submenu.classList.add('active');
              }
            }
          }
        }
        // Check if it's a direct category without subcategories (like #sports-bestof-gallery)
        else if (document.querySelector(`[data-target="${hashId}"]`)) {
          // Show the gallery directly
          showGallery(hashId);
          
          // Update mobile dropdown to match the hash
          for (const [category, galleries] of Object.entries(subcategories)) {
            const galleryFound = galleries.find(g => g.value === hashId);
            if (galleryFound) {
              sportsCategorySelect.value = category;
              const changeEvent = new Event('change');
              sportsCategorySelect.dispatchEvent(changeEvent);
              break;
            }
          }
          
          // If not found in subcategories, it's a direct category
          if (!sportsCategorySelect.value) {
            // Map gallery hashes to their categories
            const galleryToCategoryMap = {
              'sports-bestof-gallery': 'best-of',
              'sports-parapente-gallery': 'sports-aeriens',
              'sports-football-gallery': 'sports-collectifs',
              'sports-ski-freeride-gallery': 'sports-hiver',
              'sports-motocross-gallery': 'sports-mecaniques',
              'sports-trail-gallery': 'sports-terrestres'
            };
            
            const category = galleryToCategoryMap[hashId];
            if (category) {
              sportsCategorySelect.value = category;
              const changeEvent = new Event('change');
              sportsCategorySelect.dispatchEvent(changeEvent);
            }
          }
        }
      } else {
        // Show "Choisir une galerie" message when no hash
        showNoGalleryMessage();
      }
    }

    // Share gallery function
    window.shareGallery = (url) => {
      if (navigator.share) {
        navigator.share({
          title: 'Galerie Sports - Jean-Marc Favre',
          url: url
        }).catch(console.error);
      } else {
        navigator.clipboard.writeText(url).then(() => {
          alert('Lien copié dans le presse-papiers !');
        }).catch(() => {
          alert('Impossible de copier le lien. URL: ' + url);
        });
      }
    };
  });
</script>

